    <!-- Dual Focus How To Play Page -->
    <string name="how_to_play_dual_focus1">A digit blinks in the central circle along with an image in any
        one of the smaller circles.</string>
    <string name="how_to_play_dual_focus2">Tap the digit that appeared.</string>
    <string name="how_to_play_dual_focus3">Tap the image that appeared.</string>
    <string name="how_to_play_dual_focus4">abhinav</string>

    <!-- Dual Focus Pro How To Play Page -->
    <string name="how_to_play_dual_focus_pro"> Tap the smaller circle where image appeared.</string>

    <!-- Blink How To Play Page -->
    <string name="how_to_play_blink1">A stream of grey alphabets blinks along with a digit and a
        green coloured alphabet.</string>
    <string name="how_to_play_blink2">Tap that digit and the green coloured alphabet from the
        given options.</string>

    <!-- Blink Pro How To Play Page -->
    <string name="how_to_play_blink_pro1">  A stream of grey alphabets blinks with a digit,
         a green alphabet and a blue alphabet.</string>
    <string name="how_to_play_blink_pro2">Tap on the digit as well as the green and blue alphabet
        from the respective options.</string>

    <!-- Track The Route How To Play Page -->
    <string name="how_to_play_track_the_route1">The aeroplane connects the dots to form a
        particular pattern.</string>
    <string name="how_to_play_track_the_route2">Memorize the pattern and recreate it by tapping
        on the dots in the same order.</string>

    <!-- Memory Matrix How To Play Page -->
    <string name="how_to_play_memory_matrix1">Memorize the circles that glows red.</string>
    <string name="how_to_play_memory_matrix2">Tap the same circles after they disappear.</string>

    <!-- Memory Matrix Pro Pro How To Play Page -->
    <string name="how_to_play_memory_matrix_pro1">Memorize the circles that glows blue or red.</string>
    <string name="how_to_play_memory_matrix_pro2"> Tap only the red circles if indicator above
        shows red.</string>
    <string name="how_to_play_memory_matrix_pro3">Tap only the blue circles if indicator above
        shows blue.</string>

    <!-- Dancing Ball How To Play Page -->
    <string name="how_to_play_dancing_ball1">Path of the moving ball creates an outline of a shape.</string>
    <string name="how_to_play_dancing_ball2">Tap on the correct option.</string>

    <!-- Shapes How To Play Page -->
    <string name="how_to_play_shapes1">Tap on two identical shapes.</string>

    <!-- Match It How To Play Page -->
    <string name="how_to_play_match_it1"> If meaning of the text is same as the colour of the
        text, tap ‘correct’ button</string>
    <string name="how_to_play_match_it2">And if it is not same tap ‘wrong’ button.</string>

    <!-- Match It Pro How To Play Page -->
    <string name="how_to_play_match_it_pro1"> If meaning of the text in meaning box is same as
         the colour of the text in colour box tap ‘correct’ button.</string>
    <string name="how_to_play_match_it_pro2"> And if it is not same tap ‘wrong’ button.</string>

    <!-- Reversal How To Play Page -->
    <string name="how_to_play_reversal1"> When the ball is green it will move in the direction
        of your swipe.</string>
    <string name="how_to_play_reversal2">When the ball is red it will move in the direction
        opposite to your swipe.</string>
    <string name="how_to_play_reversal3"> Your objective is to put the ball in the glowing
         basket.</string>

    <!-- Reversal Pro How To Play Page -->
    <string name="how_to_play_reversal_pro1">Your objective is to put the ball in the basket of
        same colour as of the outer ring of the ball.</string>

    <!-- Money Game How To Play Page -->
    <string name="how_to_play_money_game1"> Tap ‘>‘ if the sum of coins in the left box is more.</string>
    <string name="how_to_play_money_game2"> Tap ‘>‘ if the sum of coins in the right box is more. </string>
    <string name="how_to_play_money_game3"> Tap ‘=‘ if the sum of coins in both the boxes are same.</string>

    <!-- Solve It How To Play Page -->
    <string name="how_to_play_solve_it1"> Calculate equation and answer using the keypad.</string>
    <string name="how_to_play_solve_it2">Press C to cancel your input </string>
    <string name="how_to_play_solve_it3"> Press play button to skip that equation.</string>

    <!-- After Math How To Play Page -->
    <string name="how_to_play_after_math1"> Check whether the given equation is right or wrong.</string>
    <string name="how_to_play_after_math2"> Tap ‘correct’ or ‘wrong’ button accordingly. </string>

    <!-- Speed Shop How To Play Page -->
    <string name="how_to_play_speed_shop1">Swipe the central object towards the category to
        which it belongs.</string>

    <!-- Spot It How To Play Page -->
    <string name="how_to_play_spot_it1">Spot the object passing through the binocular.</string>
    <string name="how_to_play_spot_it2">  Tap the same object from the given options.</string>

    <!-- Spot It Pro How To Play Page -->
    <string name="how_to_play_spot_it_pro1"> Spot the two objects passing simultaneously through
         the binocular.</string>
    <string name="how_to_play_spot_it_pro2">Tap the same two objects from the given options.</string>

    <!-- Brain Flash How To Play Page -->
    <string name="how_to_play_brain_flash1">Concentrate on the rotating grey circles.</string>
    <string name="how_to_play_brain_flash2">Tap the circle which changes to green as quickly as
        possible.</string>
    <string name="how_to_play_brain_flash3"> Do not tap if it turns red.</string>
